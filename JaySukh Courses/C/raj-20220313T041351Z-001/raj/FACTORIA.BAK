//to find the factorial of a given no
#include<stdio.h>
#include<conio.h>
void main()
{
	int no,i;
	long int fact=1;
	clrscr();
	printf("Enter any no");
	scanf("%d",&no);
	for(i=1;i<=no;i++)
	{
		fact=fact*i;

	}
	printf("The factorial of %d is %ld\n",no,fact);

	getch();

}
