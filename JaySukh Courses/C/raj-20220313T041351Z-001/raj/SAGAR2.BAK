//to check the given no	is armstrong or not
#include<stdio.h>
#include<conio.h>
#include"sagar.h"

void main()
{
	long int no,temp;
	int rem,sum=0;
	clrscr();
	printline();
	printf("Enter any no");
	scanf("%ld",&no);
	printanychar('*');
	temp=no;
	while(no>0)
	{
		rem=no%10;
		sum=sum+(rem*rem*rem);
		no=no/10;
	}
	printf("sum of cube of digit %d\n",sum);
	printanychar('$');
	if(sum==temp)
		printf("It is an armstrong no");
	else
		printf("It is not an armstrong no");
	printline();
	getch();

}








