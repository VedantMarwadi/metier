#include<iostream.h>
#include<conio.h>
#include<string.h>
void  swapint(int &a,int &b)
{
	int temp;
	temp=a;
	a=b;
	b=temp;
}
void  swapchar(char &ch1 ,char &ch2)
{
	char temp;
	temp=ch1;
	ch1=ch2;
	ch2=temp;
}


void main()
{
	int a=10,b=20;
	char c1='A',c2='Z';

	clrscr();
	cout<<"before swap"<<a<<"\t"<<b<<endl;
	cout<<"before swap"<<c1<<"\t"<<c2<<endl;

	swapint(a,b);
	swapchar(c1,c2);



	cout<<"after swap"<<a<<"\t"<<b<<endl;
	cout<<"after swap"<<c1<<"\t"<<c2<<endl;



	getch();
}